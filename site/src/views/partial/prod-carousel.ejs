<section class="carousel">
    <h2>Otros productos similares:</h2>
    <i class="fas fa-chevron-left"></i>
    <% similarProducts.map(element =>{ %>
    <article class="slider1">
        <img src="/img/<%= element.imagen %> " alt="">
        <h4><%= element.titulo %></h4>
        <p>$<%= element.precio %></p>
    </article>
    <% })  %>
</section>